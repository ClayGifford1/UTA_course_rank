{
  "manifest_version": 2,
  "name": "UTA Course Rank",
  "version": "2.1",

  "icons": {
	  "16": "Picture1.png",
	  "24": "Picture1.png",
	  "32": "Picture1.png"
  },

  "browser_action": {
    "default_icon": {
	  "16": "Picture1.png",
	  "24": "Picture1.png",
	  "32": "Picture1.png"
	  }
  },

  "background": {
    "page": "background.html",
    "persistent": false
  },

  "content_scripts": [{
	"matches": ["https://mymav.utshare.utsystem.edu/*"],
	"css": ["main.css"],
  "js": ["JscriptSearcher.js"]
  }],

  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'"
}